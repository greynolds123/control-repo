{
  "name": "zleslie-openldap",
  "version": "0.2.0",
  "author": "zleslie",
  "summary": "OpenLDAP Module",
  "license": "Apache-2.0",
  "source": "git://github.com/zleslie/puppet-openldap.git",
  "project_page": "https://github.com/xaque208/puppet-openldap",
  "issues_url": "https://github.com/xaque208/puppet-openldap/issues",
