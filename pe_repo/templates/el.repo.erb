[pe_repo]
name=Puppet Labs PE Packages \$releasever - \$basearch
baseurl=https://<%= @master %>:<%= @port %><%= @prefix %>/<%= @pe_version %>/<%= @installer_build %>
enabled=1
gpgcheck=1
sslverify=False
proxy=$PROXY
gpgkey=https://<%= @master %>:<%= @port %><%= @prefix %>/GPG-KEY-puppetlabs
       https://<%= @master %>:<%= @port %><%= @prefix %>/GPG-KEY-puppet
